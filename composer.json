{
    "name": "logipro/chatbot",
    "type": "app",
    "description": "start a new PHP project with a simple base structure",
    "keywords": ["php", "DDT"],
    "homepage": "https://github.com/frederic100/chatbot",
    "license": "Copyrighted",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3",
        "symfony/http-client": "^7.0",
        "phariscope/event": "^1.1",
        "symfony/http-foundation": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/orm-pack": "^2.4",
        "symfony/console": "^7.0",
        "phariscope/doctrine-testing-tools": "^1.3",
        "doctrine/cache": "^1.11",
        "symfony/monolog-bundle": "^3.10"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.28",
        "phpstan/phpdoc-parser": "^1.28",
        "phpstan/phpstan": "1.10",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "3.*",
        "behat/behat": "^3.14"
    },
    "autoload": {
        "psr-4" : {
            "Chatbot\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Chatbot\\Tests\\" : "tests/unit",
            "Chatbot\\Integration\\" : "tests/integration"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*"
        },
        "runtime": {
            "dotenv_path": "src/Infrastructure/Shared/Symfony/.env"
        }
    }
}
